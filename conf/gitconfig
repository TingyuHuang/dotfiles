[alias]

	df = diff
	dfch = diff --cached
	co = checkout
	ci = commit
	st = status
	br = branch

[core]

	editor = vim
	pager = less -x1,5

[color]

	ui = true

[color "branch"]

	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]

	meta = yellow bold
	frag = magenta bold # line info
	old = red # deletions
	new = green # additions

[color "status"]

	added = yellow
	changed = green
	untracked = cyan

